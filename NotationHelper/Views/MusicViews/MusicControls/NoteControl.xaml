<UserControl x:Class="MusicDataModel.MusicViews.NoteControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusicDataModel.MusicViews"
             mc:Ignorable="d" 
             x:Name="abc"
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
        <Ellipse Width="{Binding Diameter, ElementName=abc}" Height="{Binding Diameter, ElementName=abc}" Fill="Black"/>
    </Grid>
</UserControl>
