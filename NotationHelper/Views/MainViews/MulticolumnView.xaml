<UserControl x:Class="NotationHelper.Views.MainViews.MulticolumnView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NotationHelper.Views.MainViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             SizeChanged="UserControl_SizeChanged" Loaded="UserControl_Loaded"
             DataContextChanged="MultiColumnDataContextChanged">

    <Grid x:Name="MainGrid">
        <StackPanel x:Name="MultiColumnGrid" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="4" MinHeight="400" Orientation="Horizontal">
        
        </StackPanel>
    </Grid>
</UserControl>
